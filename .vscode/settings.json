{
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
  "github.copilot.advanced": {
    "authProvider": "github"
  },
  "github.copilot.chat.localeOverride": "ko",
  "github.copilot.chat.welcomeMessage": "never",
  "github.copilot.editor.enableAutoCompletions": true,
  "github.copilot.editor.enableCodeActions": true,
  "github.copilot.editor.iterativeFixing": true,
  "files.exclude": {
    "**/.git": false,
    "**/.svn": false,
    "**/.hg": false,
    "**/CVS": false,
    "**/.DS_Store": false,
    "**/Thumbs.db": false,
    "**/bower_components": false,
    "**/coverage": false,
    "**/tmp": false,
    "**/.mvn": false,
    "**/mvnw": false,
    "**/mvnw.cmd": false,
    "**/.git**": false,
    "**/readme.md": false,
    "**/README.md": false,
    "**/LICENSE.md": false,
    "**/CHANGELOG.md": false,
    "**/CHANGELOG": false,
    "**/.gitignore": false,
    "**/node_modules": false,
    "**/package-lock.json": false,
    "**/**build/**": false,
    "build.tar.gz": false
  },
  "explorerExclude.backup": {},
}